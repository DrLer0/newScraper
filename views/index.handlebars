<div id="wrapper">
  <div class="jumbotron">
    <h1 class="display-4">NewScraper</h1>
    <p class="lead">Get new news from CNN!</p>
  </div>
  <div class="card-columns">
    {{#each article}}
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">{{this.link}}</p>
        <a href="#" class="btn btn-primary">Save Article</a>
      </div>
    </div>
    {{/each}}
  </div>
  <div id="articles">

  </div>
  <div id="notes">

  </div>
</div>